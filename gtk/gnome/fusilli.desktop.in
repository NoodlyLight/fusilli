[Desktop Entry]
Type=Application
Encoding=UTF-8
_Name=Fusilli
Exec=fusilli
NoDisplay=true
# name of loadable control center module
X-GNOME-WMSettingsModule=fusilli
# autostart phase
X-GNOME-Autostart-Phase=WindowManager
X-GNOME-Provides=windowmanager
# name we put on the WM spec check window
X-GNOME-WMName=fusilli
# back compat only 
X-GnomeWMSettingsLibrary=fusilli
