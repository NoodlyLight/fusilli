[Desktop Entry]
Type=Application
Encoding=UTF-8
_Name=Fusilli
Exec=fusilli
NoDisplay=true

# autostart phase
X-GNOME-Autostart-Phase=WindowManager
X-GNOME-Provides=windowmanager

X-MATE-Autostart-Phase=WindowManager
X-MATE-Provides=windowmanager

# name we put on the WM spec check window
X-GNOME-WMName=fusilli
X-MATE-WMName=fusilli

# back compat only 
X-GnomeWMSettingsLibrary=fusilli
X-MateWMSettingsLibrary=fusilli
